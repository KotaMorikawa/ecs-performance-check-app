{
  "env": {
    "es2022": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "no-unused-vars": "off"
  },
  "ignorePatterns": [
    "dist",
    "node_modules",
    "prisma/migrations",
    ".eslintrc.json",
    "**/__tests__/**"
  ]
}